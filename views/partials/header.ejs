<header>



  <div class="sidebar-toggle-box">
      <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
  </div>


  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>

      <img class="jukenav" src='http://i.imgur.com/Xu8GQPK.jpg'>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <% if (user) { %>
          <li ><a href="/users/<%= user._id %>"><img id="starButton" src="http://img.freepik.com/free-vector/vinyl-disc_1021-30.jpg?size=338&ext=jpg" height="30px" width="30px"> <span class="sr-only">(current)</span></a></li>
          <% } else { %>
          <!-- <li ><a href="/users/login"><img id="starButton" src="https://www.biddingowl.com/images/signInGraphic.jpg" height="30px" width="30px"> <span class="sr-only">(current)</span></a></li> -->
          <% } %>


        </ul>
        <form class="navbar-form navbar-left" id="search" role="search" method="POST" action="/search">
          <div class="form-group">
            <input type="text" class="form-control ctn" name="searchTerm" placeholder="Search for API">
          </div>
          <button type="submit" class="btn btn-default ctn">Submit</button>
        </form>
        <ul class="nav navbar-nav navbar-right">

          <li class="dropdown">
            <% if (user) {%>
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" class="ls-nav"><%= user.local.email %><span class="caret ls-nav"></span></a>

            <ul class="dropdown-menu">


                <li><a href="/users/<%= user._id %>" class="ls-nav">Profile</a></li>
                <li><a href="/users/<%= user._id %>/edit" class="ls-nav">Account Settings</a></li>
                <li><a href="/users/logout" class="ls-nav">Log Out</a></li>
                <% } else { %>
                <li><a href="/users/login" class="ls-nav">Login</a></li>
                <li><a href="/users/signup" class="ls-nav">Signup</a></li>
                <% } %>

              <li role="separator" class="divider"></li>
            </ul>
          </li>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
</header>
